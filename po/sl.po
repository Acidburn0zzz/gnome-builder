msgid "Basic"
msgstr "Osnovno"

#: src/gstyle/gstyle-color-panel.c:952
msgid "Palette: "
msgstr "Paleta: "

#: src/gstyle/gstyle-color-panel.c:954
msgid "Palette"
msgstr "Paleta"

#: src/gstyle/gstyle-color-plane.c:1610
