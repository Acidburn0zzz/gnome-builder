#: src/gstyle/gstyle-color-panel.c:953
msgid "Palette: "
msgstr "Paleta: "

#: src/gstyle/gstyle-color-panel.c:955
msgid "Palette"
msgstr "Paleta"

#: src/gstyle/gstyle-color-plane.c:1601
